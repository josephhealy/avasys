/* Base */
body{font-family: verdana, helvetica, comic sans MS, arial, sans-serif; font-weight: normal; font-size:12px; line-height:18px; color:#666666;}
p{padding-bottom:1em;}
a{color:#666666; text-decoration: none; background-image: url(../images/link_dotted.png); background-repeat: repeat-x; background-position: bottom;}
a:active { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
a:focus { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
a:hover { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
label{font-weight:bold; font-size:13px; color:#ababab;}
strong, strong a{color:#666666;}
.highlight{color:#0B85EF;}
.attribution{color:#F7B100;}
.h1highlight{font-weight:600; color:#666666;}
.num{font-family:Georgia, Times New Roman, Serif;}
.link{cursor:pointer;}
.link:hover{text-decoration:underline;}

h1{font-family: verdana, helvetica, comic sans MS, arial, sans-serif; color:#ababab; font-size:20px; line-height:26px; font-weight:normal;}
h1 a { color: #666666; text-decoration: none; background-image: url(../images/link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
h1 a:active { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
h1 a:focus { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
h1 a:hover { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 

h1 strong{font-weight:normal; color:#666666;}
h2{font-size:10px; color: #F73101; border-bottom: 1px dotted #ababab;}b
h3{font-size:12px; color: #666666;}
h4{font-size:10px; text-transform:uppercase;}

.panel_title{font-family: verdana, georgia, helvetica, comic sans MS, arial, sans-serif; font-size:14px; line-height:18px; font-weight:600;}

/* Fade Fix */
h1, #col_2, #footer{background-color:#FFFFFF;}

/* Header */
#header{background-image: url(../images/top_divide.gif); background-position:bottom; background-repeat:repeat-x;}
#header #ds_logo #logo_text h1{font-family: "times Times New Roman", georgia, serif; font-size: 35px; line-height:40px; color: #222222}
#header #header_bug{ font-size:10px; line-height:11px; font-weight:bold;}
#header #header_bug .news_entry #title {color:#0B85EF; line-height:14px;}
#header #header_bug .news_entry .headline{text-transform:uppercase; color: #666666}
#header #header_bug .bug_item .quote_aphorism{color: #666666; text-transform: none; }

#logo_image a {text-decoration: none; background-image: none;} 
#logo_image a:active {text-decoration: none; background-image: none;} 
#logo_image a:focus {text-decoration: none; background-image: none;} 
#logo_image a:hover {text-decoration: none; background-image: none;}  
#logo_text a {text-decoration: none; background-image: none;} 
#logo_text a:active {text-decoration: none; background-image: none;} 
#logo_text a:focus {text-decoration: none; background-image: none;} 
#logo_text a:hover {text-decoration: none; background-image: none;} 

/* Navigation */
#col_1 ul#side_nav{text-transform:uppercase; font-weight: 600;  color:#666;}
#col_1 ul#side_nav a{color:#666; font-weight: 600;}
#col_1 ul#side_nav  {color:#666; font-weight: 600;}
#col_1 ul#side_nav li a{color:#999999; font-weight: 600; text-decoration:none; background-image:none;}
#col_1 ul#side_nav li ul li a{color:#666; font-weight: normal;}
#col_1 ul#side_nav li ul li .active a{text-decoration:underline;}
#col_1 ul#side_nav .all{font-size:11px;}
#col_1 ul#side_nav .spacer{border-bottom:1px dotted #ababab; border-bottom:1px dotted #ababab; width:16px; height:4px; font-size:0; line-height:0; margin-bottom:4px;}

/* page body */
#col_2 .text_block a { color: #666666; text-decoration: none; background-image: url(../images/link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
#col_2 .text_block a:active { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
#col_2 .text_block a:focus { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 
#col_2 .text_block a:hover { color: #0B85EF; text-decoration: none; background-image: url(../images/visited_link_dotted.png); background-repeat: repeat-x; background-position: bottom;} 

/* Footer*/
#footer .group a{text-decoration:none; background-image: none;}
#footer .leftgroup a{text-decoration:none; background-image: none; color:#999999; }
#footer .rightgroup a{text-decoration:none; background-image: none;color:#999999; }
#footer{border-top:5px solid #efefea; padding: 0 3px 0 3px; color:#999999; font-size:9px; font-weight:bold; text-transform:uppercase; line-height:12px;}
#footer h4{color:#0A74DE;}

/* Home */
#feature_slideshow #feature_slides {background-color:#efefea;}
#feature_slideshow #feature_slideshow_ui{background: url(../images/slideshow_ui_background_50.png); background-repeat: no-repeat;}
#feature_slideshow #feature_slideshow_ui #feature_caption{line-height:30px; margin-left:12px; color:#333333; font-weight:600;}
#feature_slideshow #feature_slideshow_ui #feature_caption a{color: #0B85EF; font-weight:600; text-decoration:none; background-image: none;}

.pager a{background-image:url(../images/pager.png)}
.pager a:hover{background-image:url(../images/pager_hover.png)}
.pager a.activeSlide{background-image:url(../images/pager_active.png)}

.pagersave a{background-color:#066696; line-height:0;}
.pagersave a:hover{background-color:#0A76CE;}
.pagersave a.activeSlide{background-color: #0C91FF;}
b
#project_group ul#project_list li{line-height:0;}
.project_caption{line-height:16px;}
.project_name{color:#666666;}
.project_thumb .project_caption {background-color:#ffffff;}

/* About */
.panel{background-image:url( ../images/diag_stripe.gif); background-repeat:repeat;}
.box{background-color:#fff;}
.box p{font-family:Georgia, Times New Roman, Serif; font-size:24px; line-height:30px; font-weight:normal; color:#ABABAB;}
.box p.var1{font-family:Georgia, Times New Roman, Serif; font-size:24px; line-height:30px; font-weight:normal; color:#666666;}

.box h3{color:#ff2525; font-size:10px;}
.box h3.var1{color:#00bfdc;}
.box h3.var2{color:#00b195;}
.box h3.var3{color:#8b0013;}
.box h3.var4{font-family:Georgia, Times New Roman, Serif; font-size:12px; color:#ababab;}
.box .details{color:#ababab;}

/* Contact */
#contact .address, #contact .address a{color:#666666;}
#contact #mapWrap #sideMap{border-top:1px dotted #ababab; border-bottom:1px dotted #ababab;}
#contact #mapWrap #sideMap .title{color:#666666;}
#contact #mapWrap #map{border: 9px solid #efefef;}

/* Projects */
.half_show .text_half .pos_statement {font-family:Georgia, Times New Roman, Serif; font-size:22px; line-height:28px; font-weight:normal;}
.half_show .text_half{border-top:1px dotted #ababab; border-bottom:1px dotted #ababab;}
.half_show .text_half .pos_statement .headline {color:#ff2525;}

.slider{width:31px; height:53px;}
#prev .slider{background-image:url(../images/arrow_l.png); background-repeat:no-repeat;}
#next .slider{background-image:url(../images/arrow_r.png); background-repeat:no-repeat;}

/* Project Details */
#work_detail .overview {font-family:Georgia, Times New Roman, Serif; font-size:31px; line-height:38px; font-weight:normal; color:#ff2525;}
#work_detail .elem_info .title {color:#666666;}
#work_detail .elem_info .description,
#work_detail .elem_show .show {border-top:1px dotted #ababab;}
#work_detail .elem_info .description {line-height:18px;}

/* software */
#software .feed .story{line-height:18px;}
#software .feed .story .date{font-size:10px;}
#software .feed .story .title a{color:#666666;}
#software .feed .story .subtitle{color:#666666; text-indent:-6px;}
#software .archive{font-size:10px; line-height:14px;}
#software .archive ul li a{color:#666666;}
#software .scroll_head{font-weight:600;}

#software .archive #scroll{background-color:#f5f5f5;}
#software .archive #scroll #shuttle{background-image:url(../images/shuttle.gif); background-repeat:no-repeat; width:6px; height:50px;}
#software .archive #window{font-family: verdana, helvetica, comic sans MS, arial, sans-serif; font-weight: normal; font-size:12px; line-height:12px; color:#666666;}

/* News */
#news .feed .story{line-height:18px;}
#news .feed .story .date{font-size:10px;}
#news .feed .story .title a{color:#666666;}
#news .feed .story .subtitle{color:#666666; text-indent:-6px;}
#news .archive{font-size:10px; line-height:14px;}
#news .archive ul li a{color:#666666;}
#news .archive #scroll{background-color:#f5f5f5;}
#news .archive #scroll #shuttle{background-image:url(../images/shuttle.gif); background-repeat:no-repeat; width:6px; height:50px;}

/* Careers */
#careers .title{font-family:Georgia, Times New Roman, Serif; color:#888888; font-size:25px; line-height:35px; font-weight:normal;}
#careers .desc{font-family:Georgia, Times New Roman, Serif; color:#888888; font-weight:normal; width:560px;}
#careers p{font-family:Georgia, Times New Roman, Serif; font-size:15px; margin-top:30px; color:#888888; font-weight:normal;}
#careers p a{color:#F73101;}

/* MISC  
----------------------------------------------------------*/
.clear{clear: both;}
.error{color:Red;}
#menucontainer{margin-top:40px;}
div#title{display:block;float:left;text-align:left;}
#logindisplay{font-size:1.1em; display:block; text-align:right; margin:10px; color:White;}
#logindisplay a:link{color: white; text-decoration: underline;}
#logindisplay a:visited{color: white; text-decoration: underline;}
#logindisplay a:hover{color: white;text-decoration: none;}

/* Styles for validation helpers
-----------------------------------------------------------*/
.field-validation-error{color: #ff0000;}
.field-validation-valid{display: none;}
.input-validation-error{border: 1px solid #ff0000; background-color: #ffeeee;}
.validation-summary-errors{font-weight: bold; color: #ff0000;}
.validation-summary-valid{display: none;}

/* Styles for editor and display helpers
----------------------------------------------------------*/
.display-label, .fieldset .editor-label{margin: 2em 0 0 0;}
.display-field, .editor-field{margin:0.5em 0 0 0; }
.text-box{width: 30em;}
.text-box.multi-line{height: 6.5em;}
.tri-state{width: 6em;}